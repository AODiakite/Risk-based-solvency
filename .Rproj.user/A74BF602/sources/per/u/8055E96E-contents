library(rvest)

# Définir l'URL de base
url_base <- "https://medias24.com/leboursier/fiche-action?action=alliances-p&valeur=historiques"

# Lire la page
page <- read_html(GET(url_base, user_agent("Mozilla/5.0 (Windows NT 10.0;Win64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/89.0.4389.82 Safari/537.36")))
page <- read_html(url_base)

# Extraire les données de la table
data <- html_table(html_node(page, 'table'), fill = TRUE)

# Afficher les données
data
