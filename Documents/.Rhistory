format_value <- function(value) {
# Remplacer la virgule par un point décimal
value <- gsub(",", ".", value, fixed = TRUE)
# Supprimer les espaces
value <- gsub(" ", "", value, fixed = TRUE)
# Insérer un espace avant le dernier point décimal
value <- gsub("(\\d)(\\.(\\d{3}))", "\\1 \\3", value)
return(value)
}
